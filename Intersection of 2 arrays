class Solution {
    public int[] intersect(int[] nums1, int[] nums2) {
        int i = 0, j = 0; 
        int m= nums1.length;
        int n  = nums2.length;
        while (i < m && j < n) { 
            if (nums1[i] < nums2[j]) 
                i++; 
            else if (nums2[j] < nums1[i]) 
                j++; 
            else { 
                System.out.print(nums2[j++] + " "); 
                i++; 
            } 
        } 
    }
}
